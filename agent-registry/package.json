{
  "name": "agent-registry",
  "version": "1.0.0",
  "description": "Cloud-based agent discovery system using vector embeddings for semantic search",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/api/server.ts",
    "start": "node dist/api/server.js",
    "build": "tsc",
    "index-agents": "ts-node scripts/index-agents.ts",
    "rebuild-test-agents": "ts-node scripts/rebuild-index-from-test-agents.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agent",
    "registry",
    "vector-search",
    "embeddings",
    "pinecone",
    "venice-ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
